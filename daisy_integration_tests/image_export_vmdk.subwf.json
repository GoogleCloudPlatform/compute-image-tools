{
  "Name": "image-export-vmdk-test",
  "Vars": {
    "source_disk_file": {
      "Required": true
    },
    "disk_name": "from-export-${ID}"
  },
  "Steps": {
    "create-disk": {
      "IncludeWorkflow": {
        "Path": "../daisy_workflows/image_import/import_disk.wf.json",
        "Vars": {
          "disk_name": "${disk_name}",
          "importer_instance_disk_size": "200",
          "source_disk_file": "${source_disk_file}"
        }
      }
    },
    "create-tester": {
      "CreateInstances": [
        {
          "disks": [{"Source": "${disk_name}"}],
          "name": "tester",
          "metadata": {
            "startup-script": "echo 'SUCCESS wVnWw3a41CVe3mBVvTMn'"
          },
          "Scopes": [
            "https://www.googleapis.com/auth/devstorage.read_write"
          ]
        }
      ]
    },
    "delete-disk": {
      "DeleteResources": {
        "Instances": ["tester"],
        "Disks": ["${disk_name}"]
      }
    },
    "verify-output": {
      "WaitForInstancesSignal": [
        {
          "Name": "tester",
          "SerialOutput": {
            "Port": 1,
            "SuccessMatch": "SUCCESS wVnWw3a41CVe3mBVvTMn"
          }
        }
      ]
    }
  },
  "Dependencies": {
    "create-tester": [
      "create-disk"
    ],
    "verify-output": [
      "create-tester"
    ],
    "delete-disk": [
      "verify-output"
    ]
  }
}
{
  "Name": "create-networks-test",
  "Vars": {
    "about-this-test": {
      "Value": "",
      "Description": "This test creates and uses a network."
    }
  },
  "Steps": {
    "create-instance": {
      "CreateInstances": [
        {
          "disks": [
            {
              "initializeParams": {
                "diskType": "pd-ssd",
                "sourceImage": "projects/debian-cloud/global/images/family/debian-10"
              }
            }
          ],
          "name": "instance",
          "networkInterfaces": [
            {
              "network": "network"
            }
          ]
        }
      ]
    },
    "create-network": {
      "CreateNetworks": [
        {
          "IPv4Range": "192.168.0.0/16",
          "name": "network"
        }
      ]
    }
  },
  "Dependencies": {
    "create-instance": [
      "create-network"
    ]
  }
}

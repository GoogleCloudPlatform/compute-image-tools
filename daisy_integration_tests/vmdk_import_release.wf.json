{
  "Name": "image-import-test",
  "Vars": {
    "image_name": {
      "Value": "image-import-test-${ID}"
    },
    "source_disk_file": {
      "Value": "gs://compute-image-tools-test-resources/xenial-server-cloudimg-amd64-disk1.vmdk"
    }
  },
  "Steps": {
    "delete-image": {
      "DeleteResources": {
        "Images": [
          "${image_name}"
        ]
      }
    },
    "import-image": {
      "Timeout": "30m",
      "IncludeWorkflow": {
        "Path": "../daisy_workflows/image_import/import_image.wf.json",
        "Vars": {
          "image_name": "${image_name}",
          "importer_instance_disk_size": "200",
          "source_disk_file": "${source_disk_file}"
        }
      }
    }
  },
  "Dependencies": {
    "delete-image": [
      "import-image"
    ]
  }
}
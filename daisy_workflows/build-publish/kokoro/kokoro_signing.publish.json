{
  "Name": "kokoro-signing",
  "WorkProject": "gce-image-builder",
  "ComputeEndpoint": "https://www.googleapis.com/compute/v1/projects/",
  "DeleteAfter": "24h*30*6",
  "PublishProject": "bct-prod-images",
  {{$time := trimPrefix .publish_version "v"}}
  "Images": [
    {
      "Prefix": "kokoro-signing-image",
      "Family": "kokoro-signing-image",
      "Description": "Image for signing RPM packages in Kokoro",
      "Licenses": [
        "projects/rocky-linux-cloud/global/licenses/rocky-linux-8"
      ],
      "GuestOsFeatures": ["UEFI_COMPATIBLE", "GVNIC"]
    }
  ]
}

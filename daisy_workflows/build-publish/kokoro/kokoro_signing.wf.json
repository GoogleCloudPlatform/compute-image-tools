{
  "Name": "kokoro-signing",
  "Project": "gce-image-builder",
  "Zone": "us-central1-b",
  "GCSPath": "gs://gce-image-build-bucket/daisy/${USERNAME}",
  "Vars": {
    "source_image_project": "bct-prod-images",
    "build_date": "${DATE}",
    "workflow_root": {"Value": "../..", "Description": "Root of github workflows, defaults to /workflows in the container."},
    "gcs_url": {"Required": true, "Description": "The GCS path that image raw file exported to."},
    "kokoro_authorized_keys": {"Required": true, "Description": "Kokoro authorized key"}
  },
  "Steps": {
    "build-bare-metal": {
      "Timeout": "70m",
      "IncludeWorkflow": {
        "Path": "../../image_build/kokoro/kokoro_signing.wf.json",
        "Vars": {
          "base_image": "projects/${source_image_project}/global/images/family/rocky-linux-8",
          "workflow_root": "${workflow_root}",
          "gcs_url": "${gcs_url}",
          "kokoro_authorized_keys": "${kokoro_authorized_keys}"
        }
      }
    }
  }
}

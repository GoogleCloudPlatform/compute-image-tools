{
  "Name": "centos-7",
  "Vars": {
    "git_revision": {"Value": "development", "Description": "The git revision/branch from compute-image-packages to be tested"},
    "git_uri": {
      "Value": "https://github.com/GoogleCloudPlatform/compute-image-packages.git",
      "Description": "The git uri reference to the compute-image-packages repository"
    },
    "alias_ip": {"Value": "10.128.3.128", "Description": "Alias IP to be used on network test (must match subnet)"}
  },
  "Steps": {
    "run": {
      "Timeout": "90m",
      "IncludeWorkflow": {
        "Path": "./package_test.subwf.json",
        "Vars": {
          "git_revision": "${git_revision}",
          "git_uri": "${git_uri}",
          "alias_ip": "${alias_ip}",
          "source_image": "projects/centos-cloud/global/images/family/centos-7",
          "build_script": "rpm_package.py"
        }
      }
    }
  }
}

{
  "Name": "import-from-image",
  "DefaultTimeout": "90m",
  "Vars": {
    "source_image": {
      "Required": true,
      "Description": "The GCE image to translate."
    },
    "disk_name": "imported-disk-${ID}"
  },
  "Steps": {
    "create-disk": {
      "CreateDisks": [{
        "Name": "${disk_name}",
        "Type": "pd-ssd",
        "ExactName": true,
        "NoCleanup": true,
        "SourceImage": "${source_image}",
        "Licenses": ["projects/compute-image-tools/global/licenses/virtual-disk-import"]
      }]
    }
  }
}

{
  "Name": "metadata-script-junk-test",
  "Vars": {
    "source_image": {"Required": true, "Description": "Image to be tested"}
  },
  "Steps": {
    "test-metadata-script-junk": {
      "SubWorkflow": {
        "Path": "./metadata-script-subwf/sub-startup-shutdown-check.subwf.json",
        "Vars": {
            "source_image": "${source_image}",
            "instance_tag": "junk",
            "startup-script-url": "https://raw.githubusercontent.com/GoogleCloudPlatform/compute-image-tools/master/daisy_workflows/image_test/metadata-script/junk.sh",
            "shutdown-script-url": "https://raw.githubusercontent.com/GoogleCloudPlatform/compute-image-tools/master/daisy_workflows/image_test/metadata-script/junk.sh",
            "startup_msg": "startup-script: INFO Found startup-script-url in metadata.",
            "shutdown_msg": "shutdown-script: INFO Found shutdown-script-url in metadata."
        }
      }
    }
  }
}
